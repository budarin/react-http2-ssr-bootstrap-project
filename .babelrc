{
    "presets": [
        [
            "env",
            {
                "loose": true,
                "useBuiltIns": true,
                "targets": {
                    "node": "current"
                }
            }
        ],
        "flow"
    ],
    "plugins": [
        "syntax-trailing-function-commas",
        "syntax-dynamic-import",
        [
            "transform-object-rest-spread",
            {
                "useBuiltIns": true
            }
        ]
    ]
}
