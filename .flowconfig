[ignore]
.*/dist/.*
.*/.idea/.*
.*/.git/.*
.*/.tmp/.*
.*/node_modules/.*

[include]
 ./src/types/**/*.js

[libs]

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

temp_dir=./.tmp/flow
suppress_comment= \\(.\\|\n\\)*\\$FlowIgnore

module.name_mapper.extension='svg' -> '<PROJECT_ROOT>/src/config/flow/stubs/url-loader.js'
module.name_mapper.extension='png' -> '<PROJECT_ROOT>/src/config/flow/stubs/url-loader.js'
module.name_mapper.extension='jpg' -> '<PROJECT_ROOT>/src/config/flow/stubs/url-loader.js'
module.name_mapper.extension='gif' -> '<PROJECT_ROOT>/src/config/flow/stubs/url-loader.js'

module.name_mapper.extension='css' -> '<PROJECT_ROOT>/src/config/flow/stub/css-modules.js'

[lints]
