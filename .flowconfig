[ignore]
.*/dist/.*
.*/.idea/.*
.*/.git/.*
.*/.tmp/.*
.*/node_modules/.*

[include]

[libs]

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
module.ignore_non_literal_requires=true
module.system.node.resolve_dirname=node_modules

temp_dir=./.tmp/flow
suppress_comment= \\(.\\|\n\\)*\\$FlowIgnore

module.name_mapper.extension='svg' -> '<PROJECT_ROOT>/src/config/flow/stubs/url-loader.js.flow'
module.name_mapper.extension='png' -> '<PROJECT_ROOT>/src/config/flow/stubs/url-loader.js.flow'
module.name_mapper.extension='jpg' -> '<PROJECT_ROOT>/src/config/flow/stubs/url-loader.js.flow'
module.name_mapper.extension='gif' -> '<PROJECT_ROOT>/src/config/flow/stubs/url-loader.js.flow'

module.name_mapper.extension='css' -> '<PROJECT_ROOT>/src/config/flow/stubs/css-modules.js.flow'

[lints]
all=error
sketchy-null-bool=off
sketchy-null-mixed=off
sketchy-null-number=off
sketchy-null-string=off
unclear-type=off
untyped-import=off
untyped-type-import=off